{
  "name": "Anki-Extension",
  "description": "Save vocabulary in your target language",
  "version": "0.01",
  "manifest_version": 3,
  "background": {
    "service_worker": "js/background.js"
  },

  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  
 "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["js/content.js"]
      }
    ]
}